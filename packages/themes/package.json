{
  "name": "@hv/uikit-common-themes",
  "version": "2.0.7",
  "description": "A collection of themes for use with Hitachi Vantara Components.",
  "homepage": "https://github.com/pentaho/hv-uikit-react",
  "license": "Apache-2.0",
  "sideEffects": false,
  "author": {
    "name": "Hitachi Vantara UI Kit Team"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pentaho/hv-uikit-react.git"
  },
  "bugs": {
    "url": "https://github.com/pentaho/hv-uikit-react/issues"
  },
  "scripts": {
    "dev": "npx npm-run-all clean -p dev:*",
    "dev:watch": "npx watch \"npm run build\" ./src/",
    "build": "npm-run-all clean build:*",
    "build:to-scss": "json-to-scss \"src/*.*\" dist",
    "build:to-json": "npx cpy-cli -f src/*.json dist",
    "clean": "npx rimraf dist hv-uikit-common-themes-*.tgz",
    "prepack": "npm run build",
    "pack": "npm pack",
    "link": "npm link",
    "license-check": "node ../../scripts/license-check.cjs ../../config/license-check.json"
  },
  "devDependencies": {
    "json-to-scss": "file:lib/json-to-scss.tgz"
  },
  "files": [
    "dist"
  ]
}
