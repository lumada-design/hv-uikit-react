{
  "name": "@hitachivantara/app-shell-navigation",
  "version": "1.4.6",
  "type": "module",
  "private": false,
  "author": "Hitachi Vantara UI Kit Team",
  "description": "AppShell Navigation",
  "homepage": "https://github.com/lumada-design/hv-uikit-react",
  "main": "src/index.ts",
  "sideEffects": false,
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lumada-design/hv-uikit-react.git",
    "directory": "packages/app-shell-navigation"
  },
  "bugs": "https://github.com/lumada-design/hv-uikit-react/issues",
  "scripts": {
    "build": "npm run clean && vite build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "clean": "npx rimraf dist package",
    "prepublishOnly": "npm run build && npx clean-publish"
  },
  "dependencies": {
    "@hitachivantara/app-shell-shared": "^1.6.0",
    "path-to-regexp": "^8.1.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-router-dom": "^6.9.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "i18next": "^25.4.2",
    "i18next-browser-languagedetector": "^8.0.3",
    "react-error-boundary": "^4.0.10",
    "react-i18next": "^15.7.2",
    "vite": "^7.1.1"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "directory": "package",
    "module": "dist/esm/index.js",
    "types": "./dist/types/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/types/index.d.ts",
        "import": "./dist/esm/index.js",
        "default": "./dist/esm/index.js"
      },
      "./package.json": "./package.json"
    }
  },
  "clean-publish": {
    "withoutPublish": true,
    "tempDir": "package",
    "fields": [
      "main"
    ],
    "files": [
      "tsconfig.json"
    ]
  }
}
