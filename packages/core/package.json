{
  "name": "@hitachivantara/uikit-react-core",
  "version": "5.0.0-next.0",
  "private": false,
  "author": "Hitachi Vantara UI Kit Team",
  "description": "UI Kit core React components for the Hitachi Vantara Design System.",
  "homepage": "https://github.com/lumada-design/hv-uikit-react",
  "main": "./src/index.ts",
  "sideEffects": false,
  "keywords": [
    "hitachi-vantara",
    "design-system",
    "ui-kit",
    "react",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lumada-design/hv-uikit-react.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/lumada-design/hv-uikit-react/issues"
  },
  "scripts": {
    "build": "pnpm clean && tsc -b tsconfig.build.json && tsc-alias",
    "test": "vitest run",
    "test:update": "vitest -u",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "clean": "npx rimraf dist",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@hitachivantara/uikit-react-icons": "workspace:*",
    "@hitachivantara/uikit-styles": "workspace:*",
    "@popperjs/core": "^2.11.6",
    "attr-accept": "^2.2.2",
    "clsx": "^1.2.1",
    "detect-browser": "^5.3.0",
    "hoist-non-react-statics": "^3.3.2",
    "lodash": "^4.17.21",
    "notistack": "^2.0.8",
    "react-google-charts": "^4.0.0",
    "react-popper": "^2.3.0",
    "react-resize-detector": "^7.1.2",
    "react-window": "^1.8.7"
  },
  "devDependencies": {
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "tsc-alias": "^1.8.2"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts"
  },
  "engines": {
    "node": ">=16.6.0"
  }
}
