{
  "name": "@hitachivantara/uikit-react-core",
  "version": "5.0.0-next.13",
  "private": false,
  "author": "Hitachi Vantara UI Kit Team",
  "description": "UI Kit core React components for the Next Design System.",
  "homepage": "https://github.com/lumada-design/hv-uikit-react",
  "main": "./src/index.ts",
  "sideEffects": false,
  "keywords": [
    "hitachi-vantara",
    "design-system",
    "ui-kit",
    "react",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lumada-design/hv-uikit-react.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/lumada-design/hv-uikit-react/issues"
  },
  "scripts": {
    "build": "run-s clean build:*",
    "build:js": "vite build",
    "test": "vitest run",
    "test:update": "vitest -u",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "clean": "npx rimraf dist package",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npx clean-publish --fields main --files tsconfig.json"
  },
  "peerDependencies": {
    "@emotion/css": "^11.10.6",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.11.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@emotion/cache": "^11.10.5",
    "@hitachivantara/uikit-react-shared": "*",
    "@hitachivantara/uikit-react-icons": "*",
    "@hitachivantara/uikit-styles": "*",
    "@popperjs/core": "^2.11.6",
    "attr-accept": "^2.2.2",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.7",
    "detect-browser": "^5.3.0",
    "hoist-non-react-statics": "^3.3.2",
    "lodash": "^4.17.21",
    "notistack": "^2.0.8",
    "react-popper": "^2.3.0",
    "react-resize-detector": "^8.0.4",
    "react-table": "^7.8.0",
    "react-window": "^1.8.8"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "tsc-alias": "^1.8.2"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "directory": "package",
    "main": "dist/cjs/index.cjs",
    "module": "dist/esm/index.js",
    "types": "dist/types/index.d.ts",
    "exports": {
      ".": {
        "require": "./dist/cjs/index.cjs",
        "import": "./dist/esm/index.js"
      }
    }
  },
  "clean-publish": {
    "withoutPublish": true,
    "tempDir": "package"
  }
}
