{
  "name": "@hitachivantara/uikit-react-icons",
  "version": "5.7.7",
  "private": false,
  "description": "NEXT Design System icons packaged as a set of React components.",
  "keywords": [
    "hitachi-vantara",
    "design-system",
    "ui-kit",
    "react",
    "typescript"
  ],
  "homepage": "https://github.com/lumada-design/hv-uikit-react",
  "bugs": {
    "url": "https://github.com/lumada-design/hv-uikit-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lumada-design/hv-uikit-react.git"
  },
  "license": "Apache-2.0",
  "author": "Hitachi Vantara UI Kit Team",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.js"
    }
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "run-s clean convert",
    "build": "vite build",
    "postbuild": "cpy \"sprites\" \"dist\"",
    "optimize": "svgo -r -f assets assets",
    "preconvert": "rimraf src",
    "convert": "run-s convert:*",
    "convert:svg": "tsx scripts/svgToReact.ts dir --input assets --output src --force",
    "convert:sprites": "tsx scripts/svgToSprite.ts dir --input assets --output sprites --force",
    "convert:copy": "cpy \"lib/*\" \"src\"",
    "clean": "rimraf dist src sprites",
    "prepare": "npm run convert"
  },
  "dependencies": {
    "@hitachivantara/uikit-styles": "^5.16.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "by-node-env": "^2.0.1",
    "cpy-cli": "^4.2.0",
    "html-to-react": "^1.7.0",
    "jsdom-no-contextify": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "recursive-readdir": "^2.2.3",
    "rimraf": "^5.0.5",
    "svgo": "^3.0.4",
    "tsx": "^3.14.0",
    "vite": "^4.5.0",
    "yargs": "^17.7.2"
  },
  "peerDependencies": {
    "@emotion/styled": "^11.10.6",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
