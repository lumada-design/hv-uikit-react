{
  "name": "@hv/uikit-react-icons",
  "version": "2.0.3",
  "description": "A collection of icons for use with Hitachi Vantara's React Components.",
  "homepage": "https://github.com/pentaho/hv-uikit-react",
  "license": "Apache-2.0",
  "author": {
    "name": "Hitachi Vantara UI Kit Team"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/pentaho/hv-uikit-react.git"
  },
  "scripts": {
    "build": "npm-run-all clean svg-optimize convert build:* copy",
    "svg-optimize": "npx svgo --config=./svgo-config.yml -r -f assets assets",
    "convert": "node ./src/svgToReact.js dir --input assets/ --output bin --force",
    "build:babel": "npx babel bin -d dist --source-maps --copy-files",
    "build:types": "npx cp src/IconBase.d.ts dist/",
    "copy": "npx cp -f bin/index.js dist/index.es.js",
    "clean": "npx del-cli dist bin",
    "prepare": "npm run build",
    "link": "npm link",
    "license-check": "node ../../scripts/license-check.cjs ../../config/license-check.json"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.8.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "@hv/uikit-common-themes": "^2.0.3",
    "clsx": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-regenerator": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.8.7",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^4.0.0",
    "cash-cp": "^0.2.0",
    "del-cli": "^3.0.0",
    "htmltojsx": "^0.3.0",
    "jsdom-no-contextify": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "recursive-readdir": "^2.2.2",
    "svgo": "^1.3.2",
    "yargs": "^15.3.0"
  },
  "files": [
    "dist"
  ]
}
