{
  "name": "@hitachivantara/uikit-react-lab",
  "version": "5.28.3",
  "private": false,
  "author": "Hitachi Vantara UI Kit Team",
  "description": "Contributed React components for the NEXT UI Kit.",
  "homepage": "https://github.com/lumada-design/hv-uikit-react",
  "main": "src/index.ts",
  "sideEffects": false,
  "keywords": [
    "hitachi-vantara",
    "design-system",
    "ui-kit",
    "react",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lumada-design/hv-uikit-react.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/lumada-design/hv-uikit-react/issues"
  },
  "scripts": {
    "build": "npm run clean && vite build",
    "test": "vitest run",
    "test:update": "vitest -u",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "clean": "npx rimraf dist package",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "run-s clean build && npx clean-publish"
  },
  "peerDependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.12.2",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^6.0.1",
    "@emotion/css": "^11.11.0",
    "@hitachivantara/uikit-react-core": "^5.55.0",
    "@hitachivantara/uikit-react-icons": "^5.8.6",
    "@hitachivantara/uikit-styles": "^5.22.0",
    "@types/react-grid-layout": "^1.3.5",
    "lodash": "^4.17.21",
    "react-grid-layout": "^1.4.4",
    "reactflow": "^11.10.1",
    "uid": "^2.0.2",
    "usehooks-ts": "^2.9.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.1",
    "@storybook/react": "^7.6.4",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "npm-run-all": "^4.1.5",
    "vite": "^5.1.0",
    "vitest": "^1.2.2"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "directory": "package",
    "main": "dist/cjs/index.cjs",
    "module": "dist/esm/index.js",
    "types": "dist/types/index.d.ts",
    "exports": {
      ".": {
        "require": "./dist/cjs/index.cjs",
        "import": "./dist/esm/index.js"
      }
    }
  },
  "clean-publish": {
    "withoutPublish": true,
    "tempDir": "package",
    "fields": [
      "main"
    ],
    "files": [
      "tsconfig.json"
    ]
  }
}
