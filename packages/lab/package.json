{
  "name": "@hitachivantara/uikit-react-lab",
  "version": "5.4.7",
  "private": false,
  "author": "Hitachi Vantara UI Kit Team",
  "description": "Contributed React components for the NEXT UI Kit.",
  "homepage": "https://github.com/lumada-design/hv-uikit-react",
  "main": "src/index.ts",
  "sideEffects": false,
  "keywords": [
    "hitachi-vantara",
    "design-system",
    "ui-kit",
    "react",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lumada-design/hv-uikit-react.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/lumada-design/hv-uikit-react/issues"
  },
  "scripts": {
    "build": "run-s clean build:*",
    "build:js": "vite build",
    "test": "vitest run",
    "test:update": "vitest -u",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "clean": "npx rimraf dist package",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npx clean-publish"
  },
  "peerDependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.12.2",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@emotion/css": "^11.11.0",
    "@emotion/serialize": "^1.1.2",
    "@hitachivantara/uikit-react-core": "^5.14.1",
    "@hitachivantara/uikit-react-icons": "^5.1.11",
    "@hitachivantara/uikit-styles": "^5.8.1",
    "clsx": "^1.2.1",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@storybook/react": "^6.5.16",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "npm-run-all": "^4.1.5",
    "vite": "^4.3.9",
    "vitest": "^0.32.0"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "directory": "package",
    "main": "dist/cjs/index.cjs",
    "module": "dist/esm/index.js",
    "types": "dist/types/index.d.ts",
    "exports": {
      ".": {
        "require": "./dist/cjs/index.cjs",
        "import": "./dist/esm/index.js"
      }
    }
  },
  "clean-publish": {
    "withoutPublish": true,
    "tempDir": "package",
    "fields": [
      "main"
    ],
    "files": [
      "tsconfig.json"
    ]
  },
  "gitHead": "c040141e323ce6aed12fd7ca98ddb3720f78141a"
}
