{
  "name": "@hv/uikit-react-core",
  "version": "0.3.0-alpha.52",
  "description": "A collection of provisional React components for the Hitachi Vantara's Design System.",
  "license": "LicenseRef-LICENSE",
  "author": {
    "name": "Hitachi Vantara UI Kit Team"
  },
  "main": "core/index.js",
  "module": "core/index.es.js",
  "repository": {
    "type": "git",
    "url": "https://10.76.48.133/hv-design-system/hv-ui.git"
  },
  "scripts": {
    "dev_win": "npm-run-all clean -p dev_win:*",
    "dev_win:watch": "watch \"npm run build\" src",
    "dev_win:storybook": "start-storybook -p 9001 -c .storybook",
    "dev": "npm-run-all clean -p dev:*",
    "dev:watch": "watch 'npm run build' src",
    "dev:storybook": "start-storybook -p 9001 -c .storybook",
    "build": "npm-run-all build:*",
    "build:copy": "copyfiles -f src/index.js core && mv core/index.js core/index.es.js",
    "build:babel": "npx babel src -d core --source-maps --copy-files",
    "build:clean": "npx del core/**/tests",
    "clean": "npx del core",
    "lint": "eslint ./ --cache --ignore-pattern .gitignore",
    "lint:fix": "eslint ./ --cache --ignore-pattern .gitignore --fix",
    "lint:jenkins": "eslint -f checkstyle . > eslint.xml",
    "test": "jest -c config/jest-config.js --coverage",
    "test:watch": "jest -c config/jest-config.js --watch",
    "test:update": "jest -c ./config/jest-config.js --u",
    "prepublish": "npm-run-all clean build",
    "postpublish": "npm run clean",
    "link": "npm link",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "pre-commit": [
    "precommit-msg",
    "lint"
  ],
