{
  "name": "@hitachivantara/uikit-react-shared",
  "version": "5.1.20",
  "private": false,
  "description": "Shared React contexts for the NEXT UI Kit.",
  "keywords": [
    "hitachi-vantara",
    "design-system",
    "ui-kit",
    "react",
    "typescript"
  ],
  "homepage": "https://github.com/lumada-design/hv-uikit-react",
  "bugs": {
    "url": "https://github.com/lumada-design/hv-uikit-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lumada-design/hv-uikit-react.git"
  },
  "license": "Apache-2.0",
  "author": "Hitachi Vantara UI Kit Team",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.js"
    },
    "./package.json": "./package.json",
    "./bundles/*": "./dist/bundles/*"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && vite build",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@hitachivantara/uikit-styles": "^5.16.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "rimraf": "^5.0.5",
    "vite": "^4.5.0"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
