{
  "name": "@hitachivantara/uikit-uno-preset",
  "version": "0.2.25",
  "type": "module",
  "private": false,
  "author": "Hitachi Vantara UI Kit Team",
  "description": "UI Kit UnoCSS preset with the NEXT theme.",
  "homepage": "https://github.com/lumada-design/hv-uikit-react",
  "main": "src/index.ts",
  "sideEffects": false,
  "keywords": [
    "hitachi-vantara",
    "design-system",
    "ui-kit",
    "typescript",
    "unocss",
    "tailwind"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lumada-design/hv-uikit-react.git",
    "directory": "packages/uno-preset"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/lumada-design/hv-uikit-react/issues"
  },
  "scripts": {
    "build": "npm run clean && vite build",
    "clean": "npx rimraf dist package",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npx clean-publish"
  },
  "peerDependencies": {
    "@unocss/core": "^0.58.5",
    "@unocss/preset-uno": "^0.58.5"
  },
  "dependencies": {
    "@hitachivantara/uikit-styles": "^5.31.0",
    "@unocss/preset-rem-to-px": "^0.58.5",
    "unocss-preset-theme": "^0.12.0"
  },
  "devDependencies": {
    "@unocss/preset-uno": "^0.58.5",
    "vite": "^5.1.0"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "directory": "package",
    "main": "dist/cjs/index.cjs",
    "module": "dist/esm/index.mjs",
    "types": "dist/types/index.d.ts",
    "exports": {
      ".": {
        "require": "./dist/cjs/index.cjs",
        "import": "./dist/esm/index.mjs"
      }
    }
  },
  "clean-publish": {
    "withoutPublish": true,
    "tempDir": "package",
    "fields": [
      "main",
      "type"
    ],
    "files": [
      "tsconfig.json"
    ]
  }
}
