{
  "name": "@hitachivantara/app-shell-shared",
  "version": "1.6.0",
  "type": "module",
  "private": false,
  "author": "Hitachi Vantara UI Kit Team",
  "description": "AppShell Shared",
  "homepage": "https://github.com/lumada-design/hv-uikit-react",
  "sideEffects": false,
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lumada-design/hv-uikit-react.git",
    "directory": "packages/app-shell-shared"
  },
  "bugs": "https://github.com/lumada-design/hv-uikit-react/issues",
  "scripts": {
    "build": "npm run clean && vite build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "clean": "npx rimraf dist package",
    "prepublishOnly": "npm run build && npx clean-publish"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-router-dom": "^6.9.0"
  },
  "optionalDependencies": {
    "@hitachivantara/uikit-react-core": "^5.101.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "i18next": "^25.4.2",
    "vite": "^7.1.1"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json",
    "./bundles/*": "./dist/bundles/*"
  },
  "publishConfig": {
    "access": "public",
    "directory": "package",
    "module": "dist/esm/index.js",
    "types": "./dist/types/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/types/index.d.ts",
        "import": "./dist/esm/index.js",
        "default": "./dist/esm/index.js"
      },
      "./package.json": "./package.json",
      "./bundles/*": "./dist/bundles/*"
    }
  },
  "clean-publish": {
    "withoutPublish": true,
    "tempDir": "package",
    "fields": [
      "main"
    ],
    "files": [
      "tsconfig.json"
    ]
  },
  "dependencies": {
    "react-i18next": "^15.7.2"
  }
}
