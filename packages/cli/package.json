{
  "name": "@hitachivantara/hv-uikit-cli",
  "version": "1.24.2",
  "private": false,
  "type": "module",
  "author": "Hitachi Vantara UI Kit Team",
  "description": "The Hitachi Vantara CLI for UI Kit apps.",
  "homepage": "https://github.com/lumada-design/hv-uikit-react",
  "license": "Apache-2.0",
  "main": "src/index.js",
  "bin": {
    "hv-uikit-cli": "src/index.js"
  },
  "keywords": [
    "hitachivantara",
    "hv-uikit-cli",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lumada-design/hv-uikit-react.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/lumada-design/hv-uikit-react/issues"
  },
  "scripts": {
    "clean": "npx rimraf package",
    "prepare": "npx cpy ../../templates src",
    "pretest": "node src/index.js create uikit-app -t Form,ListView",
    "test": "cd uikit-app",
    "posttest": "npx rimraf uikit-app",
    "prepublishOnly": "npm run clean && npx clean-publish"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "colors": "^1.4.0",
    "commander": "^10.0.0",
    "fs-extra": "^11.1.0",
    "inquirer": "^9.1.4",
    "node-plop": "^0.31.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "cpy-cli": "^5.0.0",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.3",
    "i18next-http-backend": "^3.0.2",
    "react-i18next": "15.7.1",
    "react-router-dom": "^6.29.0",
    "vite-tsconfig-paths": "^5.1.0"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=16"
  },
  "files": [
    "src"
  ],
  "publishConfig": {
    "access": "public",
    "directory": "package"
  },
  "clean-publish": {
    "withoutPublish": true,
    "tempDir": "package"
  }
}
