{
  "name": "@hitachivantara/app-shell-vite-plugin",
  "version": "1.8.9",
  "type": "module",
  "private": false,
  "author": "Hitachi Vantara UI Kit Team",
  "description": "AppShell Vite Plugin",
  "homepage": "https://github.com/lumada-design/hv-uikit-react",
  "sideEffects": false,
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lumada-design/hv-uikit-react.git",
    "directory": "packages/app-shell-vite-plugin"
  },
  "bugs": "https://github.com/lumada-design/hv-uikit-react/issues",
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.json && rollup --config",
    "dev": "tsc-watch --noClear -p tsconfig.json",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "clean": "npx rimraf dist package",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npx clean-publish"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@hitachivantara/app-shell-services": "^1.0.0",
    "@hitachivantara/app-shell-shared": "^1.6.3",
    "@hitachivantara/app-shell-ui": "^1.12.3",
    "@hitachivantara/uikit-react-icons": "^5.16.4",
    "@hitachivantara/uikit-react-shared": "^5.5.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-virtual": "^3.0.1",
    "es-module-shims": "^1.6.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "rollup": "^4.6.1",
    "ts-node": "^10.9.1",
    "vite-plugin-static-copy": "^3.1.0"
  },
  "peerDependencies": {
    "vite": "^4.1.4 || ^5.0.4 || ^6.0.0 || ^7.0.0"
  },
  "devDependencies": {
    "mock-fs": "^5.2.0",
    "tsc-watch": "^6.0.0"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "directory": "package"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "clean-publish": {
    "withoutPublish": true,
    "tempDir": "package",
    "fields": [],
    "files": [
      "tsconfig.json"
    ]
  }
}
