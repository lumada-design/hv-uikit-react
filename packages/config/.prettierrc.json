{
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],

  "importOrder": [
    "<BUILTIN_MODULES>",
    "^react$",
    "^react-.*",
    "<THIRD_PARTY_MODULES>",
    "^@hitachivantara/(.*)$",
    "",
    "^~/(.*)$",
    "^#/(.*)$",
    "^[./]"
  ],

  "overrides": [
    {
      "files": ["*.hbs", "pnpm-lock.yaml", "yarn.lock"],
      "options": { "rangeEnd": 0 }
    }
  ]
}
